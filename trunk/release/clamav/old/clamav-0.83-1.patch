+{
+  struct stat st;
+
+  DEBUG("(check_executable)  : %s\n", path ? (*path ? path : "EMPTY!") : "NULL!");
+  if ((!path) || (!*path))
+    return 0;
+
+  if ((stat (path, &st) >= 0) &&
+      (((st.st_mode & S_IXOTH) == S_IXOTH) ||
+       ((st.st_mode & S_IXGRP) == S_IXGRP) ||
+       ((st.st_mode & S_IXUSR) == S_IXUSR)))
+    return 1;
+  else
+    return 0;
+}
